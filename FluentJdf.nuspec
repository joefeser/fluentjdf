<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>FluentJdf</id>
    <version>0.1.0</version>
    <authors>Onpoint On Demand</authors>
    <owners>Onpoint On Demand</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      Fluent JDF contains everything you need to quickly
      add JDF capabilities to any client application.  It includes powerful tools
      that allow you to easily author, parse and send JDF from your client applications.
      This package uses Castle Windsor and NLog.  See NuGet for related packages
      to support other IoC/DI containers and logging frameworks.
    </description>
    <summary>JDF Client Library for .NET (Debug)</summary>
    <dependencies>
      <dependency id="NLog" version="2.0.0.1904" />
      <dependency id="Castle.Core" version="2.5.2" />
      <dependency id="Castle.Windsor" version="2.5.3" />
    </dependencies>
  </metadata>
  <files>
    <file src="BuildOutput\bin\FluentJdf\FluentJdf.dll" target="lib" />
    <file src="BuildOutput\bin\FluentJdf\FluentJdf.xml" target="lib" />
    <file src="BuildOutput\bin\FluentJdf\Infrastructure.Core.dll" target="lib" />
    <file src="BuildOutput\bin\FluentJdf\Infrastructure.Container.CastleWindsor.dll" target="lib" />
    <file src="BuildOutput\bin\FluentJdf\Infrastructure.Logging.NLog.dll" target="lib" />
  </files>
</package>